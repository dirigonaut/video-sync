{
  "name": "Video-Sync",
  "version": "0.2.0",
  "main": "index.js",
  "scripts": {
    "install": "sudo apt-get install redis-server ffmpeg -y && sudo systemctl disable redis-server && sudo npm install && npm run-script init",
    "init": "node develop/init.js",
    "start": "node index.js",
    "debug": "npm run-script init && node --inspect=9999 index.js",
    "test": "echo testing tbd",
    "stats": "cloc --not-match-d resources --by-file ./src/ ./static/ index.js ./static/css/icons/",
    "update-deps": "npm outdated --depth=0 | grep -v Package | awk '{print $1}' | xargs -I% npm install %@latest --save"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "browserify": "^14.1.0",
    "ebml": "^2.2.0",
    "express": "^4.15.2",
    "find": "^0.2.7",
    "marked": "^0.3.6",
    "moment": "^2.14.1",
    "node-forge": "^0.7.0",
    "redis": "^2.7.1",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "winston": "^2.3.1",
    "xml2js": "^0.4.17",
    "xmldom": "^0.1.27"
  },
  "description": "An application for synchronizing a media experience across multiple clients.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:dirigonaut/video-sync.git"
  },
  "author": "Sabo",
  "license": "The MIT License",
  "homepage": "https://github.com/dirigonaut/video-sync.git#readme"
}
